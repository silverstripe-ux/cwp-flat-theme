<form $FormAttributes class="input-group search-form" role="search">
	<input id="SearchForm_SearchForm_Search" class="search-query form-control input-sm" type="search" name="Search" placeholder="SEARCH" value="$SearchQuery" title="Enter search terms" />
	<span class="input-group-btn">
		<button type="submit" class="btn btn-default"><i class="flat-icon-search"></i><span class="sr-only">Go</span></button>
	</span>
</form>